import java.util.Scanner;
public class oddarr
{
	public static void main(String args[])
	{
		
		Scanner sc=new Scanner(System.in);
		int numbers[]={2,1,3,4,5,6};
		
			
		for (int i=0;i<numbers.length;i++)
		{
			
			
				if(numbers[i]%2!=0)
				{
					System.out.println("The odd nos are "+numbers[i]);
				}
		}
	}

}
